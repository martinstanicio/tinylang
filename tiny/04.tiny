program p4 .
var
counter : int (0 ... 100) = 0 ;
done : bool = false ;
begin
let counter = counter + 1 ;
if counter < 10 goto loop ; else goto salida ;
loop : let done = not done ;
goto counterCheck ;
counterCheck : if done == true goto endloop ; else goto loop ;
endloop : let counter = counter * 2 ;
salida : let done = not false ;
end
